    "@xmcl/file-transfer": "^1.0.3",
    "@xmcl/installer": "^5.1.0",
    "@xmcl/task": "^4.0.6",
    "@xmcl/user": "^3.0.3",
    "axios": "^1.4.0",
    "decompress": "^4.2.1",
    "electron-browser-storage": "^1.0.7",
    "electron-root-path": "^1.1.0",
    "electron-updater": "^5.3.0",
    "isomorphic-fetch": "^3.0.0",
    "minecraft-launcher-core": "^3.17.2",
    "minecraft-launcher-js": "^0.1.0",
