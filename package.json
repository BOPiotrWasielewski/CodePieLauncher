    "@vue/eslint-config-prettier": "^7.1.0",
    "electron": "^24.6.2",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.25",
    "sass": "^1.66.1",
    "vite": "^4.4.2",
    "vue": "^3.3.4"
  }
